<component name="GoogleAnalytics" extends="Task">
  <interface>
    <!-- Public -->
    <function name="initialize" />
    <function name="setConfig" />
    <function name="setOptions" />
    <function name="start" />
    <function name="logEvent" />
    <function name="logScreenView" />
    <function name="setUserProperties" />
